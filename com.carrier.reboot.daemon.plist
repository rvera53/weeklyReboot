<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.atos.reboot.daemon</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Library/Scripts/ForceRebootDaemon.sh</string>
    </array>
    
    <key>StartInterval</key>
    <integer>600</integer> <key>StandardErrorPath</key>
    <string>/var/log/com.carrier.WeeklyRebootNotifier.log</string>
    <key>StandardOutPath</key>
    <string>/var/log/com.carrier.WeeklyRebootNotifier.log</string>
</dict>
</plist>